{
  "builds": [
    {
      "src": "src/skogul/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.14" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": ".vercel/output" }
    }
  ],
  "routes": [
    {
      "src": "/src/static/(.)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/src/(.)",
      "dest": "skogul/wsgi.py"
    }
  ]
}